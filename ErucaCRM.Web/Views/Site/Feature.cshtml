@model ErucaCRM.Web.ViewModels.PublicFeaturePageVM
@using ErucaCRM.Web.Infrastructure;
@using ErucaCRM.Utility;



@{
    ViewBag.Title = Model.CultureSpecificPageMetaTitle;
    ViewBag.MetaTag = Model.CultureSpecificPageMetaTags;
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="cultureContentblock">
    <div class="cultureContent">

        @Html.Raw(string.IsNullOrWhiteSpace(Model.CultureSpecificPageContent) ? "" : Model.CultureSpecificPageContent.Replace("&nbsp;", " "))
    </div>
</div>
